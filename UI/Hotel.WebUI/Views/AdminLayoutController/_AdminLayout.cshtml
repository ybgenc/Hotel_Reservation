
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

    @await Html.PartialAsync("/Views/AdminLayout/HeadPartial.cshtml")

<body>

    <!--Preloader start-->
    @await Html.PartialAsync("/Views/AdminLayout/PreLoaderpartial.cshtml")
    <!--Preloader end-->

    <!--Main Wrapper Start-->
    <div id="main-wrapper">

        <!--Nav header start-->
        @await Html.PartialAsync("/Views/AdminLayout/NavHeaderPartial.cshtml")
        <!--Nav header end-->

        <!--Header start-->
        @await Html.PartialAsync("/Views/AdminLayout/HeaderPartial.cshtml")
        <!--Header end-->

        <!--Sidebar start-->
        @await Html.PartialAsync("/Views/AdminLayout/SideBarPartial.cshtml")
        <!--Sidebar end-->

        <!--Content body start-->
        <div class="content-body">

            <div class="row page-titles mx-0">
                <div class="col p-md-0">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="javascript:void(0)">Dashboard</a></li>
                        <li class="breadcrumb-item active"><a href="javascript:void(0)">Home</a></li>

                    </ol>
                </div>
            </div>
            <!-- row -->

            <div class="container-fluid">
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                                @RenderBody()
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- #/ container -->
        </div>
    
        <!--Content body end-->

        <!--Footer start-->
        @await Html.PartialAsync("/Views/AdminLayout/FooterPartial.cshtml")
        <!--Footer start-->
    </div>
    <!--Main Wrapper End-->

    <!--Scripts start-->
    @await Html.PartialAsync("/Views/AdminLayout/ScriptsPartial.cshtml")
    <!--Scripts end-->

</body>

</html>

