
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

    @await Html.PartialAsync("/Views/AdminLayout/HeadPartial.cshtml")

<body>

    <!--Preloader start-->
    @await Html.PartialAsync("/Views/AdminLayout/PreLoaderpartial.cshtml")
    <!--Preloader end-->

    <!--Main Wrapper Start-->
    <div id="main-wrapper">

        <!--Nav header start-->
        @await Html.PartialAsync("/Views/AdminLayout/NavHeaderPartial.cshtml")
        <!--Nav header end-->

        <!--Header start-->
        @await Html.PartialAsync("/Views/AdminLayout/HeaderPartial.cshtml")
        <!--Header end-->

        <!--Sidebar start-->
        @await Html.PartialAsync("/Views/AdminLayout/SideBarPartial.cshtml")
        <!--Sidebar end-->

        <!--Content body start-->
        @await Html.PartialAsync("/Views/AdminLayout/ContentBodyPartial.cshtml")
        @RenderBody()
        <!--Content body end-->

        <!--Footer start-->
        @await Html.PartialAsync("/Views/AdminLayout/FooterPartial.cshtml")
        <!--Footer start-->
    </div>
    <!--Main Wrapper End-->

    <!--Scripts start-->
    @await Html.PartialAsync("/Views/AdminLayout/ScriptsPartial.cshtml")
    <!--Scripts end-->

</body>

</html>

