
@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">

<!--HEAD-->
@await Component.InvokeAsync("_HeadPartial")
<!--HEAD-->

<body>
    <div  class="container-xxl bg-white p-0 mt-0">
        <!-- Spinner Start -->
        @await Component.InvokeAsync("_SpinnerPartial")
        <!-- Spinner End -->

        <!-- Header Start -->
        @await Component.InvokeAsync("_HeaderPartial")
        <!-- Header End -->


        <!-- Carousel Start -->
        @await Component.InvokeAsync("_CarouselPartial")
        <!-- Carousel End -->


        <!-- Booking Start -->
        @await Component.InvokeAsync("_BookingPartial")
        <!-- Booking End -->


        <!-- About Start -->
        @await Component.InvokeAsync("_AboutPartial")
        <!-- About End -->


        <!-- Room Start -->
        @await Component.InvokeAsync("_RoomPartial")
        <!-- Room End -->


        <!-- Video Start -->
        @await Component.InvokeAsync("_VideoPartial")
        <!-- Video end -->


        <!-- Service Start -->
        @await Component.InvokeAsync("_ServicePartial")
        <!-- Service End -->


        <!-- Testimonial Start -->
        @await Component.InvokeAsync("_TestimonialPartial")
        <!-- Testimonial End -->


        <!-- Team Start -->
        @await Component.InvokeAsync("_StaffPartial")
        <!-- Team End -->


        <!-- Newsletter Start -->
        @await Html.PartialAsync("_SubscribePartial.cshtml")
        <!-- Newsletter Start -->
        

        <!-- Footer Start -->
        @await Component.InvokeAsync("_FooterPartial")
        <!-- Footer End -->


        <!-- Back to Top -->
        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
    </div>

    <!-- Scripts Start -->
    @await Component.InvokeAsync("_ScriptsPartial")
    <!-- Scripts End -->

</body>

</html>
